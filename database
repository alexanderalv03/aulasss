CREATE DATABASE cadastroalunos;

use cadastroalunos;

CREATE TABLE alunos(
id int auto_increment primary key,
nome varchar(50),
idade varchar(50),
semestre varchar(50),
genero varchar(50),
matricula varchar(50)
);

insert into alunos(nome, idade, semestre,genero,matricula) values('Rafael','22','Segundo','Masculino','11111');
insert into alunos(nome, idade, semestre, genero, matricula) values('Rafaela','25', 'Primeiro','Feminino','22222');


select * from alunos;
